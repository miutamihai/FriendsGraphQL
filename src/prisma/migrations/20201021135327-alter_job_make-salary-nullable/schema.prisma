datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Friend {
  id Int @id @default(autoincrement())
  firstName String
  lastName String
  jobId Int @default(1)
  job Job @relation(fields: [jobId], references: [id])
}

model Job {
  id Int @id @default(autoincrement())
  name String
  monthlyPay Int?
}
